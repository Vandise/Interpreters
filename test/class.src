
class Person

  // classes generally have memeber functions
  func greet(name)
    print("Hello " + name)
  end

end

class User : Person

  func construct()
    self
  end

  func login(name)
    greet(name)
  end

end

current_user = User.new
current_user.login("Ben")

// <User 0x...>
print(current_user.class_instance)
// User
print(current_user.class_name)